# PDO DSN, username, password
SetEnv DSN mysql:dbname=yutar
SetEnv DBUSER yutar
SetEnv DBPASS yutar

# Session encoding; shuffle the text!
SetEnv CODED 0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz-._

# URL transformation
RewriteEngine On
RewriteRule ^attachment/([0-9]+)/(.+)$ attachment/$1-$2 [L]
RewriteRule ^[^.]+$ index.php [L]
